<div class="form-group row" id="engrow2" style="padding: 0px">
    <div class="form-group col-md-2">
        <% if(formvalues.engein2.length > 0) { %>
        <input style="padding: 0px" type="number" class="form-control" name="engein2" id="engein2" value="<%=formvalues.engein2%>">
        <% } else { %>
        <input style="padding: 0px" type="number" class="form-control" name="engein2" id="engein2" placeholder="Enter engineer EIN" value="">
        <% } %>
    </div>

    <div class="form-group col-md-2">
        <select name="tasknum2" id="tasknum2" class="form-control">
            <option value="" selected><em>Select From List</em></option>
            <%for(var oo =0;oo<dropdownsjson.tasknumberdropdown.length;oo++) { %>
            <% if( dropdownsjson.tasknumberdropdown[oo].TASK_NUMBER == formvalues.tasknum2) { %>
            <option value="<%=dropdownsjson.tasknumberdropdown[oo].TASK_NUMBER%>" selected><em><%=dropdownsjson.tasknumberdropdown[oo].TASK_NUMBER%></em></option>
            <% } else { %>
            <option value="<%=dropdownsjson.tasknumberdropdown[oo].TASK_NUMBER%>"><em><%=dropdownsjson.tasknumberdropdown[oo].TASK_NUMBER%></em></option>
            <% } %>
            <% } %>
        </select>
    </div>

    <div class="form-group col-md-2">
        <select name="traveltime2" id="traveltime2" class="form-control">
            <option value="" selected><em>Select From List</em></option>
            <%for(var mm =0;mm<dropdownsjson.traveldropdown.length;mm++) { %>
            <% if( dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION == formvalues.travel2) { %>
            <option value="<%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%>" selected><em><%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%></em></option>
            <% } else { %>
            <option value="<%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%>"><em><%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%></em></option>
            <% } %>
            <% } %>
        </select>
    </div>

    <div class="form-group col-md-2" id="randomdiv">
        <select name="eodtravel2" id="eodtravel2" class="form-control">
            <option style="padding: 0px" value="" selected><em>Select From List</em></option>
            <%for(var mm =0;mm<dropdownsjson.traveldropdown.length;mm++) { %>
            <% if( dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION == formvalues.eodtravel2) { %>
            <option style="padding: 0px" value="<%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%>" selected><em><%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%></em></option>
            <% } else { %>
            <option style="padding: 0px" value="<%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%>"><em><%=dropdownsjson.traveldropdown[mm].PLANNED_TT_DURATION%></em></option>
            <% } %>
            <% } %>
        </select>
    </div>

    <div class="form-group col-md-2">
        <% if(formvalues.dates2.length > 0) { %>
        <input type="text" class="auto-kal" data-kal="mode: 'multiple', months: 1, direction: 'today-future'" name="dates2" id="dates2" value = "<%=formvalues.dates2%>" readonly>
        <% } else { %>
        <input type="text" class="auto-kal" data-kal="mode: 'multiple', months: 1, direction: 'today-future', blackout: function(date) { var today = new Date(); today.getDate(); var plus30 = today.setMonth(today.getMonth()+1); return date > plus30} " name="dates2" id="dates2" readonly>
        <% } %>
    </div>

    <div class="form-group col-md-1">
        <% if (formvalues.notesarray[2].length > 0) { %>
        <a href="javascript:" id="notelink2">
            <span style="text-indent: 25px" class="glyphicon glyphicon-file" id ="eng2notes" name="noteicon"></span>
        </a>
        <% } else { %>
        <a href="javascript:" id="notelink2">
            <span style="text-indent: 25px" class="glyphicon glyphicon-plus" id ="eng2notes" name="noteicon"></span>
        </a>
        <% } %>
    </div>

    <div class="form-group col-md-1" >
        <input class="form-check-input" type="checkbox" id="deleteeng2" name="deleteeng2" value="Y" style="padding-left: 10px">
    </div>

    <% if (formvalues.notesarray[2].length > 0 ) { %>
    <div id="note2dialog" title="Notes" style="display: none;">
        <textarea id="note2" name="note2" maxlength="255" style="width:550px; height:250px;"><%= formvalues.notesarray[2] %></textarea>
    </div>
    <%} else { %>
    <div id="note2dialog" title="Notes" style="display: none;">
        <textarea id="note2" name="note2" maxlength="255" style="width:550px; height:250px;"></textarea>
    </div>
    <% } %>

</div>